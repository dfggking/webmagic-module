<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <properties resource="jdbc.properties"></properties>
  
  <classPathEntry location="${jdbc.driverLocation}"/>
  <context id="context1" targetRuntime="MyBatis3">
    
    <commentGenerator>
      <!-- 去除自动生成的注释 -->
      <property name="suppressAllComments" value="true"/>
    </commentGenerator>
    
    <!-- 数据库连接配置 -->
    <jdbcConnection
      driverClass="${jdbc.driver}"
      connectionURL="${jdbc.url}"
      userId="${jdbc.username}"
      password="${jdbc.password}"/>
    
    <!-- 非必需，类型处理器，在数据库类型和java类型之间的转换控制-->
    <javaTypeResolver>
      <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>
    
    <!--配置生成的实体包
        targetPackage：生成的实体包位置，默认存放在src目录下
        targetProject：目标工程名
     -->
    <javaModelGenerator targetPackage="com.webmagic.model" targetProject="src/main/java"/>
    
    <!-- 实体包对应映射文件位置及名称，默认存放在src目录下 -->
    <sqlMapGenerator targetPackage="com.webmagic.mapper.xml" targetProject="src/main/java"/>
    
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.webmagic.mapper" targetProject="./src/main/java">
      <property name="enableSubPackages" value="false"/>
    </javaClientGenerator>
    
    <!-- 配置表
        schema：不用填写
        tableName: 表名
        enableCountByExample、enableSelectByExample、enableDeleteByExample、enableUpdateByExample、selectByExampleQueryId：
        去除自动生成的例子
    -->
    <table schema="" tableName="user" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="user_login" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="role" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="role_menu" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="menu" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="information" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="member" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="sys_config" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="sys_dic_item" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="sys_dic_type" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="sys_login_log" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="sys_operator_log" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="user_role" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="web_config" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    <table schema="" tableName="website_config" enableCountByExample="false" enableSelectByExample="false"
           enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
  </context>
</generatorConfiguration>