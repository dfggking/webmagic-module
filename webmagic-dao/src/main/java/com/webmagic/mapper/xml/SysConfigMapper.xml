<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webmagic.mapper.SysConfigMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.webmagic.model.SysConfig">
        <id column="id" property="id" />
        <result column="file_save_position" property="fileSavePosition" />
        <result column="web_url" property="webUrl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_save_position, web_url
    </sql>

</mapper>
