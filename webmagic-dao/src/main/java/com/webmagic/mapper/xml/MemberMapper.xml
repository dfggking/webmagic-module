<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webmagic.mapper.MemberMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.webmagic.model.Member">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="major1" property="major1" />
        <result column="major2" property="major2" />
        <result column="blog_url" property="blogUrl" />
        <result column="avatar_url" property="avatarUrl" />
        <result column="institute" property="institute" />
        <result column="is_show" property="isShow" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, major1, major2, blog_url, avatar_url, institute, is_show
    </sql>

</mapper>
