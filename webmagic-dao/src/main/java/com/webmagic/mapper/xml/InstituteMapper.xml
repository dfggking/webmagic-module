<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webmagic.mapper.InstituteMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.webmagic.model.Institute">
        <id column="id" property="id" />
        <result column="introduction" property="introduction" />
        <result column="main_img" property="mainImg" />
        <result column="img1" property="img1" />
        <result column="img2" property="img2" />
        <result column="img3" property="img3" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, introduction, main_img, img1, img2, img3
    </sql>

</mapper>
