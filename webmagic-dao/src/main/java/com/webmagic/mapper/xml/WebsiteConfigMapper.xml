<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webmagic.mapper.WebsiteConfigMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.webmagic.model.WebsiteConfig">
        <id column="id" property="id" />
        <result column="website_title" property="websiteTitle" />
        <result column="keyword" property="keyword" />
        <result column="description" property="description" />
        <result column="email" property="email" />
        <result column="copyright" property="copyright" />
        <result column="address" property="address" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, website_title, keyword, description, email, copyright, address
    </sql>

</mapper>
